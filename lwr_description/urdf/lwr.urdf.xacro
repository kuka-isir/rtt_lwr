<?xml version="1.0" encoding="UTF-8"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"
      xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
   name="lwr" >
   <xacro:property name="velocity_scale" value="50.0"/>
   <xacro:property name="effort_scale" value="50.0"/>

   <xacro:property name="M_PI" value="3.1415926535897931" />

   <xacro:property name="damping_factor" value="1.0"/>

   <xacro:property name="d0" value="${damping_factor*5.5}"/>
   <xacro:property name="d1" value="${damping_factor*5.5}"/>
   <xacro:property name="d2" value="${damping_factor*3.5}"/>
   <xacro:property name="d3" value="${damping_factor*3.5}"/>
   <xacro:property name="d4" value="${damping_factor*3.5}"/>
   <xacro:property name="d5" value="${damping_factor*1.1}"/>
   <xacro:property name="d6" value="${damping_factor*0.1}"/>




  <!-- Gazebo joint configuration -->

   <xacro:macro name="lwr" params="parent *origin ">

      <!-- joint between {parent} and lwr_0_link -->
      <joint name="joint_base" type="fixed">

         <xacro:insert_block name="origin" />
         <parent link="${parent}" />
         <child link="link_0" />
      </joint>
      <link name="link_0">
          <inertial>
            <origin
              xyz="-0.000638499331014356 5.02538509694617E-06 0.0482289968116927"
              rpy="0 0 0" />
            <mass
              value="1.21032454350876" />
            <inertia
              ixx="0.00262560565710656"
              ixy="-5.2754950052563E-07"
              ixz="3.77940202490646E-05"
              iyy="0.00280724642508563"
              iyz="-2.56972470148208E-07"
              izz="0.00306998250407766" />
          </inertial>
         <visual>
            <origin xyz="0 0 0" rpy="0 0 ${M_PI}" />
            <geometry>
               <mesh filename="package://lwr_description/meshes/lwr/link0.stl" />
            </geometry>
            <material name="color_j0" />
         </visual>
         <collision>
            <origin xyz="0 0 0" rpy="0 0 ${M_PI}" />
            <geometry>
               <mesh filename="package://lwr_description/meshes/lwr/link0_c2.stl" />
            </geometry>
         </collision>
      </link>
      <!-- joint between lwr_0_link and lwr_1_link -->
      <joint name="joint_0" type="revolute">

         <parent link="link_0" />
         <child link="link_1" />
         <limit lower="${-170 * M_PI / 180}" upper="${170 * M_PI / 180}" effort="${effort_scale * 200}" velocity="${velocity_scale * 112.5 * M_PI / 180}" />

         <dynamics friction="0" damping="${d0}"/>
         <origin xyz="0 0 0.102" rpy="0 0 0" />
         <axis xyz="0 0 1" />
      </joint>
      <link name="link_1">
          <inertial>
            <origin
