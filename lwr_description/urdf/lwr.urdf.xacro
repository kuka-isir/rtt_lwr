              rpy="0 0 0" />
            <mass
              value="2.30339938771869" />
            <inertia
              ixx="0.0156081163626041"
              ixy="5.97319920503909E-08"
              ixz="-1.64780770629425E-07"
              iyy="0.0153467542173805"
              iyz="0.00319168949093809"
              izz="0.00440736079943446" />
